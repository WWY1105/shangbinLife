.container {
    padding: 0 ;
    padding-bottom: 60px;
    overflow-x: hidden;
    background-color: #fa6450;
}
.title {
  width: 86%;
  padding: 40rpx 20rpx 50rpx;
  margin: 50rpx 32rpx 0;
  border-radius: 5px;
  background-color: #fffdf6;
}
.bg {
  width: 100%;
  position: relative;
}
.bg .logo {
  width: 100%;
  height: 430rpx;
}

.logo_title {
  width: 100%;
  height: 120rpx;
  position: absolute;
  bottom: 0rpx;
  left: 0;
}
.inlogo {
  display: block;
  border: 1px solid #fff;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin: 0 auto;
}
.nickname {
  width: 100%;
  display: block;
  margin: 30rpx auto;
  font-size: 35rpx;
  color: #000;
  text-align: center;
  font-weight: bold;
}

.touse {
  margin-top: 35rpx ;
  width: 100%;
  display: block;
  text-align: center;
  font-size: 28rpx;
  color: #c32a12;
}
.title .user {
  color: #000;
}
.title .user text {
  border-bottom: 1px solid #c32a12;
  color: #c32a12;
}
.number {
  margin: 0 auto;
  width: 70%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 56rpx;
  color: #fff;
  box-shadow: 0 5px 10px 0 rgba(255, 56, 47, 0.5);
  background-image: linear-gradient(to bottom, #ff7e4f, #ff7c4f);
}
.over {
  margin-top: 30rpx;
  background : #ccc;
  color: #fff;
  box-shadow: none;
  
}
.list {
  margin-top: 30rpx;
  margin-bottom: 15rpx;
  color: #fff;
  width: 750rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  display: block;
  text-align: center;
  background: url(https://i.ishangbin.com/img/user/images/list_bg1.png) center no-repeat;
  background-size: 100% 25%;
}
.coupontitle {
  width: 100%;
  position: relative;
}

.coupons {
  width: 100%;
  padding-bottom: 40rpx;
}
.coupon {
  margin: 10rpx auto;
  background :url(https://i.ishangbin.com/img/user/images/coupon_grey.png) center no-repeat;
  background-size: 100% ;
  font-size: 20rpx;
  position: relative;
  width: 626rpx;
  height: 200rpx;
  padding: 40rpx 48rpx 40rpx;
  /* box-shadow: 0 5rpx 15rpx -5rpx rgba(0, 0, 0, 0.2); */
}
.coupon image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin-right: 32rpx;
  display: inline-block;
}
.coupon .pointpic{
  border-radius: 0
}
.right {
  float: right;
  width: 380rpx;
}
.right .point {
  line-height: 200rpx;
  font-size: 36rpx;
  font-weight: 500;
  letter-spacing: 0.3px;
  color: #000000;
}
.right .name {
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  width: 380rpx;
  max-height: 70rpx;
  font-size: 36rpx;
  font-weight: 500;
  letter-spacing: 0.3px;
  color: #000000;
  overflow: hidden;
  padding-bottom: 30rpx;
  border-bottom: 1px solid #e5e5e5;
  
}
.right .useStrategy::before , .right .times::before{
 content: ".";
 display: block;
 position: absolute;
 top: -5rpx;
 /* transform: translateY(50%); */
 left: 0;
}
.right .useStrategy {
  /* padding-top: 15rpx; */
  height: 34rpx;
  font-size: 24rpx;
  letter-spacing: 0.2px;
  color: #999999;
  position: relative;
  padding-left: 15rpx;
  
}
.right .times {
  height: 34rpx;
  font-size: 24rpx;
  letter-spacing: 0.2px;
  color: #999999;
  position: relative;
  padding-left: 15rpx;
  
}
.tips {
  position: fixed;
  top: 0;
  left: 0;
   width: 100%;
  height: 100%;
}
.inviters {
  width: 100%;
  height: 100%;
  opacity: 0.7;
  background-color: #000000;
}
.Show {
  display: block;
}
.Hide {
  display: none ;
}
.Rectangle-8 {
  padding-bottom: 40rpx;
  border-radius: 5px;
  background-color: #ffffff;
  position: absolute;
  top: 10%;
  left: 0;
  right : 0;
  margin:  150rpx auto;
  width: 70%;
  text-align: center;
  padding:  0 25rpx 30rpx;
}

.title3 {
  text-align: center;
  height: 85rpx;
  line-height: 85rpx;
  position: relative;
}

.titleText {
  font-size: 16px;
  font-weight: 500;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: 0.3px;
  color: #333333;
}
.message {
  font-size: 32rpx;
  text-align: center;
}
.know {
  margin-top: 30rpx;
  padding-top: 20rpx;
  font-size: 32rpx;
  color: #db2b32;
  height: 52rpx;
  line-height: 50rpx;
  border-top: 1px solid #ccc;
  
}

.user {
  width: 100%;
  padding-bottom: 50rpx;
}
.user .item {
  border: 1px solid #ffdec4;
  border-radius: 5px;
  margin: 0 32rpx 30rpx;
  position: relative;
}
.more {
  width: 218rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  box-shadow: 0 5px 10px 0 rgba(255, 0, 0, 0.2);
  background-image: linear-gradient(to bottom, #feefe1, #ffdec4);
  font-size: 24rpx;
  font-weight: 500;
  letter-spacing: 0.7px;
  text-align: center;
  color: #fa6450;
  position: absolute;
  left: 50%;
  bottom: 0;
  transform:translateX(-50%) translateY(50%);
}
.first {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #FFE400;
}
.useritem {
  margin: 0 auto;
  height: 100rpx;
  line-height: 80rpx;
  padding: 15rpx 30rpx 15rpx;
  position: relative;
}
.userlogo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  float: left;
}
.username {
  margin-left: 15rpx;
  width: 80%;
  font-size: 28rpx;
  display: inline-block;
  color: #fff;
}
.usertime {
  font-size: 28rpx;
  position: absolute;
  top: 40%;
  transform: translateY(-50%);
  right: 20rpx;
  /* color: #fca598; */
  color: #FDD0CA;
  display: inline-block;
}

.brand {
  width: 100%;
  color: #fff;
  font-size: 32rpx;
}
.brand image {
  width: 100%;
}

.introduction {
  width: 90%;
  margin: 0 auto;
  margin-bottom: 40rpx;
  font-size: 28rpx;
  
}
.slogan {
  font-size: 40rpx;
  font-weight: bold;
  width: 90%;
  margin: 0 auto;
  margin-bottom: 25rpx;
  text-align: center;
}
.shops{
  width: 90%;
  margin: 30rpx auto;
  font-size: 24rpx;
  letter-spacing: 0.8px;
  color: #ffffff;
  
}
.shopname {
  width: 444rpx;
  display: inline-block;
  font-size: 30rpx;
  font-weight: 500;
  letter-spacing: 1px;
}
.tel {
  float: right;
  text-align: right;
  color: #FDD0CA;
  
}
.address {
  margin-top: 15rpx;
  display: block;
  /* opacity: 0.7; */
  color: #FDD0CA;
  
}
.tab {
  height: 128rpx;
  position: fixed;
  right: 0;
  bottom: 100rpx;
}
.tab .show {
  height: 128rpx;
}

.tab .show  .close {
  width: 100rpx;
  height: 66rpx;
  border-radius: 50rpx 0  0 50rpx;
  box-shadow: -4px 0 10px 0 rgba(0, 0, 0, 0.2);
  background-color: #ffffff;
  padding-top: 36rpx;
  font-size: 24rpx;
  letter-spacing: 0.2px;
  text-align: center;
  color: #333333;
  display: inline-block;
  vertical-align: middle;
  
}
.tab .show  .close image {
  width: 56rpx;
  height: 56rpx;
  display: block;
  margin: 0 auto;
  margin-top: -30rpx;
}